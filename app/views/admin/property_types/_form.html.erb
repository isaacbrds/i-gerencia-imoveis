<%= simple_form_for [:admin, @property_type] do |form| %>
  <%= render(CardComponent.new(title: resource_form_title)) do |componente| %>
      <%= form.input :name %>

      <% componente.with_footer do %>
        <%= form.submit "Enviar", class: 'btn btn-success' %>
      <% end %>  
  <% end %>
<% end %>
  
